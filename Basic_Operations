#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
  int num1,num2;
  cin>>num1>>num2;
  cout<<"Add: "<<num1+num2<<",Subtract: "<<num1-num2<<
  ",Multiply: "<<num1*num2<<",Integer Division: "<<num1/num2<<",Mod: "
  <<num1%num2<<",Fractional Division: "<<num1/(1.0*num2)<<endl;
  return 0;
}
